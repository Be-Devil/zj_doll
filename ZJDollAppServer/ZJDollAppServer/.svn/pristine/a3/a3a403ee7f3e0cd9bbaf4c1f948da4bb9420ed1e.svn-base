Second = this.remainSecond - 1;\n        let day = Math.floor(this.remainSecond / (60 * 60 * 24));\n        let hour = Math.floor(this.remainSecond / (60 * 60)) - (day * 24);\n        let minute = Math.floor(this.remainSecond / 60) - (day * 24 * 60) - (hour * 60);\n        let second = Math.floor(this.remainSecond) - (day * 24 * 60 * 60) - (hour * 60 * 60) - (minute * 60);\n        if (this.remainSecond > 0) {\n          let d = String(day);\n          let m = String(minute);\n          let s = String(second);\n          let h = String(hour);\n          this.day = d; // Math.floor(this.remainSecond / (60 * 60 * 24));\n          this.hour = h; // Math.floor(this.remainSecond / (60 * 60)) - (day * 24);\n          this.minute = m; //Math.floor(this.remainSecond / 60) - (day * 24 * 60) - (hour * 60);\n          this.second = s; // Math.floor(this.remainSecond) - (day * 24 * 60 * 60) - (hour * 60 * 60) - (minute * 60);\n        }\n      }\n    },\n    changeImg: function() {\n      this.imgChange = !this.imgChange;\n    },\n    jumpCharge: function() {\n      if (this.isShare) {\n        httpUtils.jumpDownloadUrl();\n      }else{\n        window.location.href = \"http://t.xiehou360.com/6@#\" + 0;\n      }\n    },\n    jumpDownload: function() {\n      httpUtils.jumpDownloadUrl();\n    },\n    reload: function() {\n      window.location.reload();\n    },\n  }\n}\n\n</script>\n<style>\n\n.r_footer_block{\n  width: 100%;\n  height:2rem;\n}\n.r_footer{\n      position: fixed;\n    bottom: 0;\n}\n.down{\n    position: fixed;\n    width:100%;\n    left:0;\n    bottom:0;\n}\n.footer{\n    margin-bottom:1rem;\n}\n\n\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/DollCharge.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('section',{staticClass:\"maxBox\"},[_c('div',{staticClass:\"top\"},[_c('img',{attrs:{\"src\":\"http://lianai-image-sys.qiniudn.com/mq/bgTop.png\",\"alt\":\"\"}}),_vm._v(\" \"),_c('button',{staticClass:\"payb