String = url.substring(url.indexOf(\"?\") + 1, url.length).split(\r\n      \"&\");\r\n    var params = {}\r\n    for (i = 0; j = paraString[i]; i++) {\r\n      params[j.substring(0, j.indexOf(\"=\")).toLowerCase()] = j.substring(\r\n        j.indexOf(\"=\") + 1, j.length);\r\n    }\r\n    var returnValue = params[paramName.toLowerCase()];\r\n    if (typeof(returnValue) == \"undefined\") {\r\n      return \"\";\r\n    } else {\r\n      return returnValue;\r\n    }\r\n  };\r\n\r\n  //读取cookies \r\n  var getCookie = function(name) {\r\n    var arr, reg = new RegExp(\"(^| )\" + name + \"=([^;]*)(;|$)\");\r\n    //alert(arr,reg);\r\n    if (arr = document.cookie.match(reg))\r\n\r\n      return unescape(arr[2]);\r\n\r\n    else\r\n      return null;\r\n  };\r\n\r\n  function setCookie(name, value) {\r\n    var Days = 30;\r\n    var exp = new Date();\r\n    exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);\r\n    document.cookie = name + \"=\" + escape(value) + \";expires=\" + exp.toGMTString();\r\n  }\r\n\r\n\r\n  var isWeixinVisit = function() {\r\n    var ua = navigator.userAgent;\r\n    var isWeixin = /MicroMessenger/i.test(ua);\r\n    return isWeixin;\r\n  }\r\n  var isMobile = function() {\r\n    var sUserAgent = navigator.userAgent.toLowerCase();\r\n    var bIsIpad = sUserAgent.match(/ipad/i) == \"ipad\";\r\n    var bIsIphoneOs = sUserAgent.match(/iphone os/i) == \"iphone os\";\r\n    var bIsMidp = sUserAgent.match(/midp/i) == \"midp\";\r\n    var bIsUc7 = sUserAgent.match(/rv:1.2.3.4/i) == \"rv:1.2.3.4\";\r\n    var bIsUc = sUserAgent.match(/ucweb/i) == \"ucweb\";\r\n    var bIsAndroid = sUserAgent.match(/android/i) == \"android\";\r\n    var bIsCE = sUserAgent.match(/windows ce/i) == \"windows ce\";\r\n    var bIsWM = sUserAgent.match(/windows mobile/i) == \"windows mobile\";\r\n    if (bIsIpad || bIsIphoneOs || bIsMidp || bIsUc7 || bIsUc || bIsAndroid || bIsCE || bIsWM) {\r\n      return true;\r\n    } else {\r\n      return false;\r\n    }\r\n  }\r\n  var isShareVisit = function() {\r\n    var shareFlag = request('shareFlag');\r\n    if (shareFlag == 1) {\r\n      return true;\r\n    }\r\n    return false;\r\n  }\r\n\r\n  var jumpDownloadUrl = function() {\r\n    window.location.href = downloadUrl;\r\n  }\r\n\r\n\r\n  return {\r\n    getBaseUrl: getBaseUrl,\r\n    request: request,\r\n    getCookie: getCookie,\r\n    setCookie: setCookie,\r\n    isWeixinVisit: isWeixinVisit,\r\n    isShareVisit: isShareVisit,\r\n    getDownloadUrl: getDownloadUrl,\r\n    jumpDownloadUrl: jumpDownloadUrl,\r\n    isMobile: isMobile,\r\n  };\r\n});\r\n\n\n\n// WEBPACK FOOTER //\n// ./src/common/httpUtils.js","/*基础http通用接口*/\r\ndefine([], function() {\r\n    //var baseUrl = \"http://www.nuannuanzhibo.com/\";\r\n    var baseUrl=\"http://www.mengquww.com/\";\r\n    var downloadUrl = \"http://www.mengquww.com/download/gw\";\r\n\r\n    var getBaseUrl = function() {　　　　\r\n        var url = location.href;\r\n        if (url.indexOf(\"mengquww.com\") != -1) {\r\n            baseUrl = \"http://www.mengquww.com/\";\r\n        } else if (url.indexOf(\"t.xiehou360.com\") != -1) {\r\n            baseUrl = 'http://122.11.48.251:6639/DollAppServer/';\r\n        }else {\r\n            baseUrl = '/';\r\n        }\r\n        return baseUrl;\r\n    };\r\n\r\n\r\n    var getDownloadUrl = function() {　　　　\r\n        return downloadUrl;\r\n    };\r\n\r\n    var request = function(paramName) {\r\n        var url = location.href;\r\n        var paraString = url.substring(url.indexOf(\"?\") + 1, url.length).split(\r\n            \"&\");\r\n        var params = {}\r\n        for (i = 0; j = paraString[i]; i++) {\r\n            params[j.substring(0, j.indexOf(\"=\")).toLowerCase()] = j.substring(\r\n                j.indexOf(\"=\") + 1, j.length);\r\n        }\r\n        var returnValue = params[paramName.toLowerCase()];\r\n        if (typeof(returnValue) == \"undefined\") {\r\n            return \"\";\r\n        } else {\r\n            return returnValue;\r\n        }\r\n    };\r\n\r\n    //读取cookies \r\n    var getCookie = function(name) {\r\n        var arr, reg = new RegExp(\"(^| )\" + name + \"=([^;]*)(;|$)\");\r\n        //alert(arr,reg);\r\n        if (arr = document.cookie.match(reg))\r\n\r\n            return unescape(arr[2]);\r\n\r\n        else\r\n            return null;\r\n    };\r\n\r\n    function setCookie(name, value) {\r\n        var Days = 30;\r\n        var exp = new Date();\r\n        exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);\r\n        document.cookie = name + \"=\" + escape(value) + \";expires=\" + exp.toGMTString();\r\n    }\r\n\r\n\r\n    var isWeixinVisit = function() {\r\n        var ua = navigator.userAgent;\r\n        var isWeixin = /MicroMessenger/i.test(ua);\r\n        return isWeixin;\r\n    }\r\n\r\n    var isShareVisit = function() {\r\n        var shareFlag=request('shareFlag');\r\n        if (shareFlag==1) {\r\n            return true;\r\n        }\r\n        return false;\r\n    }\r\n\r\n    var jumpDownloadUrl = function() {\r\n       window.location.href = downloadUrl;\r\n    }\r\n    return {\r\n        getBaseUrl: getBaseUrl,\r\n        request: request,\r\n        getCookie: getCookie,\r\n        setCookie: setCookie,\r\n        isWeixinVisit: isWeixinVisit,\r\n        isShareVisit:isShareVisit,\r\n        getDownloadUrl:getDownloadUrl,\r\n        jumpDownloadUrl:jumpDownloadUrl,\r\n    };\r\n});\n\n\n// WEBPACK FOOTER //\n// ./src/assets/js/httpUtils.js","/*基础http通用接口*/\r\ndefine([], function() {\r\n  //var baseUrl = \"http://www.nuannuanzhibo.com/\";\r\n  var getDateAdd = function(AddDayCount) {\r\n    var dd = new Date();\r\n    dd.setDate(dd.getDate() + AddDayCount); //获取AddDayCount天后的日期 \r\n    var y = dd.getFullYear();\r\n    var m = dd.getMonth() + 1; //获取当前月份的日期 \r\n    var d = dd.getDate();\r\n    return y + \"-\" + m + \"-\" + d;\r\n  }\r\n\r\n  var getDateAddByString = function(dateStr, AddDayCount) {\r\n    var dd = convertDateFromString(dateStr);\r\n    dd.setDate(dd.getDate() + AddDayCount); //获取AddDayCount天后的日期 \r\n    var y = dd.getFullYear();\r\n    var m = dd.getMonth() + 1; //获取当前月份的日期 \r\n    var d = dd.getDate();\r\n    return y + \"-\" + m + \"-\" + d;\r\n  }\r\n\r\n  var convertDateFromString = function(dateString) {\r\n    if (dateString) {\r\n      var arr1 = dateString.split(\"-\");\r\n      var date = new Date(arr1[0], parseInt(arr1[1]) - 1, arr1[2]);\r\n      return date;\r\n    } else {\r\n      return new Date();\r\n    }\r\n  }\r\n\r\n  var getTimestamp = function() {\r\n    // 获取当前时间戳(以s为单位)\r\n    var timestamp = Date.parse(new Date());\r\n    timestamp = timestamp / 1000;\r\n    return timestamp;\r\n  }\r\n\r\n  function fmtDate(obj) {\r\n    let dobj=obj;\r\n    let date = new Date(parseInt(dobj));\r\n    var y = 1900 + date.getYear();\r\n    var m = \"0\" + (date.getMonth() + 1);\r\n    var d = \"0\" + date.getDate();\r\n    var h= \"0\" +date.getHours(); \r\nvar minute= \"0\" +date.getMinutes(); \r\nvar second= \"0\" +date.getSeconds(); \r\n    return y + \"-\" + m.substring(m.length - 2, m.length) + \"-\" + d.substring(d.length - 2, d.length)+\" \"+h.substring(h.length - 2, h.length)+\":\"+minute.substring(minute.length - 2, minute.length)+\":\"+second.substring(second.length - 2, second.length);\r\n  }\r\n\r\n  var reload = function() {\r\n    window.location.reload();\r\n  }\r\n\r\n\r\n\r\n\r\n  return {\r\n    getDateAdd: getDateAdd,\r\n    convertDateFromString: convertDateFromString,\r\n    getDateAddByString: getDateAddByString,\r\n    getTimestamp: getTimestamp,\r\n    reload: reload,\r\n    fmtDate:fmtDate,\r\n  };\r\n});\r\n\n\n\n// WEBPACK FOOTER //\n// ./src/common/common.js","const axios = require('axios');\r\nconst qs = require('qs');\r\nconst httpUtils = require('./httpUtils.js');\r\n\r\naxios.defaults.timeout = 5000; //响应时间\r\naxios.defaults.headers.post['Content-Type'] = 'application/x-www-form-urlencoded;charset=UTF-8'; //配置请求头\r\naxios.defaults.baseURL = httpUtils.getBaseUrl(); //配置接口地址\r\n//POST传参序列化(添加请求拦截器)\r\naxios.interceptors.request.use((config) => {\r\n    //在发送请求之前做某件事\r\n    if (config.method === 'post') {\r\n        config.data = JSON.stringify(config.data);\r\n        console.info(\"request config.data\", config.data);\r\n    }\r\n    return config;\r\n}, (error) => {\r\n    console.info(\"错误的传参\", 'fail');\r\n    return Promise.reject(error);\r\n});\r\n//返回状态判断(添加响应拦截器)\r\n/*axios.interceptors.response.use((res) => {\r\n    //对响应数据做些事\r\n    if (!res.state) {\r\n        // _.toast(res.data.msg);\r\n        return Promise.reject(res);\r\n    }\r\n    return res;\r\n}, (error) => {\r\n    console.info(\"网络异常\", 'fail');\r\n    return Promise.reject(error);\r\n});*/\r\n//返回一个Promise(发送post请求)\r\nvar fetch = function(url, params) {\r\n    console.info('url:'+url);\r\n    console.info('params:'+params);\r\n    return new Promise((resolve, reject) => {\r\n        axios.post(url, params)\r\n            .then(response => {\r\n                resolve(response.data);\r\n            }, err => {\r\n                reject(err);\r\n            })\r\n            .catch((error) => {\r\n                reject(error)\r\n            })\r\n    })\r\n}\r\n\r\nexport {fetch }\n\n\n// WEBPACK FOOTER //\n// ./src/common/httpCoreUtils.js","<template>\n  <section class=\"maxBox\">\n    <div class=\"top\">\n      <img src=\"http://lianai-image-sys.qiniudn.com/mq/bgTop.png\" alt=\"\"/>\n      <button class=\"paybtn\" @click=\"jumpCharge\"></button>\n      <!--倒计时-->\n      <div class=\"top_time\" v-if=\"remainSecond>0\">\n        <h5>距离活动结束还有</h5>\n        <p><strong class=\"top_day\"><s></s><span class=\"time\">{{day<10?'0':day[0]}}</span><span class=\"time\">{{day<10?day:day[1]}}</span></strong>天</p>\n        <p><strong class=\"top_hour\"><s id=\"h2\"></s><span class=\"time\">{{hour<10?'0':hour[0]}}</span><span class=\"time\">{{hour<10?hour:hour[1]}}</span></strong>时</p>\n        <p><strong class=\"top_minute\"><s></s><span class=\"time\">{{minute<10?'0':minute[0]}}</span><span class=\"time\">{{minute<10?minute:minute[1]}}</span></strong>分</p>\n        <p><strong class=\"top_second\"><s></s><span class=\"time\">{{second<10?'0':second[0]}}</span><span class=\"time\">{{second<10?second:second[1]}}</span></strong>秒</p>\n      </div>\n      <!--倒计时-->\n    </div>\n    <!--排行榜-->\n    <div class=\"listBox\">\n      <!--  -->\n      <!--我的排名-->\n\n      <div class=\"list_me\"  v-for=\"(item,index) in list\" v-if=\"index==0&&!isShare\">\n            <div class=\"list_ranking\">未上榜</div><!--排名-->\n            <div class=\"list_pic\"><img :src=\"item.head\" alt=\"\"/></div><!--头像-->\n            <div class=\"list_name\">{{item.name}}</div>\n            <div class=\"list_number\">福气值&nbsp;|&nbsp;<em>{{item.value}}</em></div>\n        </div>\n      <!--用户排名-->\n      <ul class=\"user_list\">\n        <li v-for=\"(item,index) in list\" v-if=\"index>0\">\n          <div class=\"user_listBox\">\n            <div class=\"list_ranking user_ranking\"><i>{{index}}</i></div>\n            <!--排名-->\n            <div class=\"list_pic\">\n              <img :src=\"item.head\" alt=\"\" />\n              <i v-if=\"index==1\"><img src=\"http://lianai-image-sys.qiniudn.com/mq/pic_1.png\" alt=\"\"/></i>\n              <!--皇冠-->\n              <i v-if=\"index==2\"><img src=\"http://lianai-image-sys.qiniudn.com/mq/pic_2.png\" alt=\"\"/></i>\n              <!--皇冠-->\n              <i v-if=\"index==3\"><img src=\"http://lianai-image-sys.qiniudn.com/mq/pic_3.png\" alt=\"\"/></i>\n              <!--皇冠-->\n            </div>\n            <!--头像-->\n            <div class=\"list_name\">{{item.name}}</div>\n            <!--昵称-->\n            <div class=\"list_number\">福气值&nbsp;|&nbsp;<em>{{item.value}}</em></div>\n            <!--福气值-->\n          </div>\n        </li>\n      </ul>\n    </div>\n    <!--排行榜-->\n    <div class=\"footer\"></div>\n    <button class=\"refurbish\" @click=\"reload\"></button>\n    <!--刷新按钮-->\n    <!-- <div class=\