		<script src="https://cdn.jsdelivr.net/npm/vue@2.5.16/dist/vue.min.js"></script>
		<script src="https://cdn.bootcss.com/axios/0.18.0/axios.min.js"></script>
		<script src="https://cdn.bootcss.com/Swiper/4.2.6/js/swiper.min.js"></script>
		<script>
			function GetRequest() {
				var url = location.search; //获取url中"?"符后的字串 
				var theRequest = {};
				if(url.indexOf("?") != -1) {
					var str = url.substr(1);
					strs = str.split("&");
					for(var i = 0; i < strs.length; i++) {
						theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
					}
				}
				return theRequest;
			}

			function q(s) {
				return document.querySelectorAll(s)
			}

			function s(s) {
				return document.querySelector(s)
			}
			var b = q('.niudan-btn');
			for(var i = 0; i < b.length; i++) {
				b[i].style.height = b[i].offsetWidth + 'px';
			}

			var app = new Vue({
				el: '#app',
				data: {
					baseUrl: 'http://zengjjingstore.realgamecloud.com/niudan/', // 资源路径
					machineId: '', //扭蛋机ID
					uid: '',
					loginKey: '',
					isCanNiu: 1,
					marquee: 0,
					giftModal: 0,
					giftModalShow: 0,
					niudanBg: 1,
					shake: 0,
					kucun: 0,
					price: 0,
					mute: 0,
					startMp3: null,
					giftUrl: '',
					gifts: [],
					records: [],
					goonStyle: {},
					showGiftPanel: 0,
					showRecordPanel: 0,
					loaded: 0,
					rewardInfos: [], //扭蛋奖品结果
					giftName: '',
					optId: '1',
					times: 0,
					yuE: 0, //余额
					swiper2: 0,
					isTry: 1,
					shiguangjuan: 0, //时光卷
					timer: null,
					timerNumber: 25
				},
				created: function() {
					var vm = this;
					var query = GetRequest();
					vm.uid = query.uid;
					vm.loginKey = query.loginKey;
					vm.machineId = query.machineId;
					axios.post('http://www.happycooltt.com/api/eggMachine/getAccountInfo', {
						uid: vm.uid,
						loginKey: vm.loginKey
					}, {
						headers: {
							'Content-Type': 'application/x-www-form-urlencoded'
						}
					}).then(function(res) {
						if(res.data.state) {
							vm.yuE = res.data.accountInfo.amount;
							vm.shiguangjuan = res.data.accountInfo.num;
							vm.loaded++;
							if(vm.loaded == 2) {
								s('.loader').style.display = 'none';
							}

						} else {
							alert(res.data.msg);
						}
					}).catch(function(res) {
						alert(res.data.msg);
					});
					axios.post('http://www.happycooltt.com/api/eggMachine/getEggMachineInfo', {
						uid: vm.uid,
						loginKey: vm.loginKey,
						machineId: vm.machineId
					}, {
						headers: {
							'Content-Type': 'application/x-www-form-urlencoded'
						}
					}).then(function(res) {
						if(res.data.state) {
							vm.giftUrl = res.data.eggMachineInfo.detailUrls[0]; //奖品第一个图片
							vm.gifts = res.data.eggMachineInfo.detailUrls; //所有奖品数组
							vm.giftName = res.data.eggMachineInfo.name; //奖品名称
							vm.kucun = res.data.eggMachineInfo.inventory; //库存
							vm.price = res.data.eggMachineInfo.price; //价格
							vm.$nextTick(function() {
								var swiper = new Swiper('#swiper1', {
									pagination: {
										el: '.swiper-pagination',
									}
								});
							});
							vm.loaded++;
							if(vm.loaded == 2) {
								s('.loader').style.display = 'none';
							}
						} else {
							alert(res.data.msg);
						}
					}).catch(function(res) {
						alert(res.data.msg);
					});

				},
				mounted: function() {
					document.body.style.backgroundImage = "url(" + this.baseUrl + "gacha-bg.jpg)";
					this.goonStyle = {
						backgroundImage: "url(" + this.baseUrl + "gacha-result-btn.png)"
					};
				},
				methods: {
					niu: function(e, times, type) { //type 0: 金币 1：时光卷
						var vm = this;
						if(!vm.kucun > 0) {
							alert('库存不足！');
							return;
						}
						if(!vm.isCanNiu) {
							return;
						}
						vm.startMp3 = document.getElementById('startMp3');
						vm.isTry = times ? 0 : 1;
						vm.isCanNiu = 0;
						vm.niudanBg = 0;
						vm.times = times;
						if(!vm.mute) {
							vm.startMp3.play()
						}
						if(e) {
							e.target.parentNode.style.transform = 'rotate(90deg)';
						} else if(vm.times == 1) {
							q('.niudan-btn')[2].style.transform = 'rotate(90deg)';
						} else if(vm.times == 3) {
							q('.niudan-btn')[3].style.transform = 'rotate(90deg)';
						} else if(vm.times == 10) {
							q('.niudan-btn')[1].style.transform = 'rotate(90deg)';
						}
						vm.sendNiudanHttp(times, type);

					},
					niuAnimation: function(fn) {
						var vm = this;
						setTimeout(function() {
							vm.shake = 1;
							vm.marquee = 1;
						}, 500);
						setTimeout(function() {
							vm.shake = 0;
							q('.dan')[0].style.top = '0';
							q('.dan')[0].style.opacity = 1;
							q('.dan')[0].style.transform = 'rotate(-360deg)';
						}, 3000);
						setTimeout(function() {
							q('.dan')[0].style.display = 'none';
							setTimeout(function() {
								fn(vm);
								vm.reset();
							}, 50)
						}, 5200);
					},
					sendNiudanHttp(times, type) {
						var vm = this;
						axios.post('http://www.happycooltt.com/api/eggMachine/play', {
							uid: vm.uid,
							loginKey: vm.loginKey,
							isTry: times == 0 ? 1 : 0,
							machineId: vm.machineId,
							type: type,
							time: times,
							optId: vm.optId
						}, {
							headers: {
								'Content-Type': 'application/x-www-form-urlencoded'
							}
						}).then(function(res) {
							if(vm.swiper2) {
								vm.swiper2.destroy();
							}
							if(res.data.state) {
								vm.niuAnimation(function(vm) {
									vm.giftModal = 1;
									vm.rewardInfos = res.data.rewardInfos;
									vm.optId = res.data.optId;
									if(!vm.isTry && !type) { // 非尝试情况
										vm.kucun = res.data.inventory;
										vm.yuE -= vm.price * times;
										vm.timerNumber = 30;
										vm.timer = setInterval(function() {
											vm.timerNumber--;
											if(vm.timerNumber == 0) {

												vm.closeModal(0);
											}
										}, 1000);
									}

									vm.shiguangjuan = res.data.num;
									vm.$nextTick(function() {
										vm.swiper2 = new Swiper('#swiper2', {
											pagination: {
												el: '.swiper-pagination',
											}
										});
									});

								})
							} else {
								vm.reset();
								alert(res.data.msg);
							}

						}).catch(function(res) {
							alert(res.data.msg);
						});
					},
					reset: function() {
						var vm = this;
						var btns = null;
						vm.niudanBg = 1;
						vm.giftModalShow = 1;
						vm.isCanNiu = 1;
						vm.marquee = 0;
						q('.dan')[0].style.top = '-100%';
						q('.dan')[0].style.opacity = .5;
						q('.dan')[0].style.transform = 'rotate(0deg)';
						btns = q('.niudan-btn');
						for(var i = 0; i < btns.length; i++) {
							btns[i].style.transform = 'rotate(0deg)';
						}
					},
					goOn: function() {
						var vm = this;
						q('.dan')[0].style.display = 'block';
						vm.giftModal = 0;
						vm.giftModalShow = 0;
						if(+vm.shiguangjuan > 0) {
							vm.niu(false, vm.times, 1);
						} else {
							vm.niu(false, vm.times, 0);
						}
					},
					closeModal: function(t) {
						var vm = this;
						if(!t) {
							vm.getReward(1);
							clearInterval(vm.timer)
						}
						q('.dan')[0].style.display = 'block';
						vm.giftModal = 0;
						vm.giftModalShow = 0;
					},
					getReward: function() {
						var vm = this;

						axios.post('http://www.happycooltt.com/api/eggMachine/getRewards', {
							uid: vm.uid,
							loginKey: vm.loginKey,
							optId: vm.optId
						}, {
							headers: {
								'Content-Type': 'application/x-www-form-urlencoded'
							}
						}).then(function(res) {
							console.log(res.data)
						})
					}
				}
			});
		</script>
	</body>

</html>