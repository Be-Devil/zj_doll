<script>
var request = function(paramName) {
    var url = location.href;
    var paraString = url.substring(url.indexOf("?") + 1, url.length).split(
        "&");
    var params = {}
    for (i = 0; j = paraString[i]; i++) {
        params[j.substring(0, j.indexOf("=")).toLowerCase()] = j.substring(
            j.indexOf("=") + 1, j.length);
    }
    var returnValue = params[paramName.toLowerCase()];
    if (typeof(returnValue) == "undefined") {
        return "";
    } else {
        return returnValue;
    }
};
var uid = request('uid');
var code = request('itemCode');
var openId = request('openId');
var unionId = request('unionId');
(function(win) {
    $(".downBtn").click(function() {
        window.location = "http://www.mengquww.com/download/gw";
    });

    $(".pay_list li").each(function(i, domEle) {
        $(this).click(function() {
            $(this).children("span").addClass("hotlist");
            $(this).siblings("li").children("span").removeClass("hotlist");
        });
    });
    uid = request('uid');
    code = request('itemCode');
    openId = request('openId');
    unionId = request('unionId');
    getList(uid, code);

})(window);


$(".selectBtn_one").click(function() {
    $(".toast").fadeIn().delay(1000).fadeOut(3000);
});



function doConfig() {
    var shareTitel = "亲爱哒，我有句话想跟你说……";
    var shareDesc = "你在我心里总是那么特别那么重要。";
    var shareUrl = "http://www.mengquww.com/api/oauth/wxAuth/pay4me?appid=wxc68eccf842b1eeb0&userInfoMark=userInfoMark&uid=" + uid + "&itemCode=" + code;
    var head = "http://sglive.imlianai.com/cmsdoll/20171209095029515";
    var visit_qq = "http://www.mengquww.com/api/oauth/wxAuth/pay4me?appid=wxc68eccf842b1eeb0&userInfoMark=userInfoMark&uid=" + uid + "&itemCode=" + code;
    $.getJSON('http://www.mengquww.com/api/wechatSupport/info', {
        url: document.location.href,
        type: 2
    }, function(data) {
        if (data.state) {
            var res = data.js;
            wx.config({
                debug: false,
                appId: res.appId,
                timestamp: res.timestamp,
                nonceStr: res.nonceStr,
                signature: res.signature,
                jsApiList: ['onMenuShareTimeline',
                    'onMenuShareAppMessage', 'onMenuShareQQ'
                ]
            });

            //getChargeOrder(uid,code,1);
            wx.ready(function() {
                wx.onMenuShareAppMessage({
                    title: shareTitel,
                    desc: shareDesc,
                    link: shareUrl,
                    imgUrl: head,
                    success: function() {}
                });

                wx.onMenuShareTimeline({
                    title: shareTitel,
                    link: shareUrl,
                    imgUrl: head,
                    success: function() {

                    }

                });
                wx.onMenuShareQQ({
                    title: shareTitel,
                    desc: shareDesc,
                    link: visit_qq,
                    imgUrl: head,
                    success: function() {}
                });

            });

        }
    });
}


function onBridgeReady(appId, timeStamp, nonceStr, package, paySign) {
    WeixinJSBridge.invoke('getBrandWCPayRequest', {
        appId: appId, //公众号名称，由商户传入     
        timeStamp: timeStamp, //时间戳，自1970年以来的秒数     
        nonceStr: nonceStr, //随机串     
        package: package,
        signType: "MD5", //微信签名方式：     
        paySign: paySign
        //微信签名 
    }, function(res) {
        //alert(res.err_msg);
        if (res.err_msg == "get_brand_wcpay_request:ok") {} // 使用以上方式判断前端返回,微信团队郑重提示：res.err_msg将在用户支付成功后返回    ok，但并不保证它绝对可靠。 
    });
}
