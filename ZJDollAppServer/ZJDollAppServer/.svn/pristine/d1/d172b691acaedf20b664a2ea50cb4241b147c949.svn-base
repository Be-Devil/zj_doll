            }
            //return iAppPushCoinRemoteService.finish(vo.getUid(),vo.getBusId(),vo.getCustomerId());
            return new BaseRespVO(0, false, vo.getMsg());
        }
        return new BaseRespVO(0, false, "下机失败:机器不存在");
    }

