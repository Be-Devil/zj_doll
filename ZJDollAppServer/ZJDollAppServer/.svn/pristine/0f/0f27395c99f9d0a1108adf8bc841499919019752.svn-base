package com.imlianai.zjdoll.app.modules.support.exchange.vo;

import com.imlianai.rpc.support.common.BaseModel;

public class ShopUserNotice extends BaseModel{

	private long uid;
	
	private long ts;
	
	private int isRead;

	public long getUid() {
		return uid;
	}

	public void setUid(long uid) {
		this.uid = uid;
	}

	public long getTs() {
		return ts;
	}

	public void setTs(long ts) {
		this.ts = ts;
	}

	public int getIsRead() {
		return isRead;
	}

	public void setIsRead(int isRead) {
		this.isRead = isRead;
	}
	
	
}
