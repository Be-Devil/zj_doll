package com.imlianai.zjdoll.app.modules.core.push.vo;

import com.imlianai.rpc.support.common.cmd.BaseRespVO;

/**
 * @author wurui
 * @create 2018-05-25 17:31
 **/
public class BoxValueRespVO extends BaseRespVO {

    private int total = 1000;
    private int value = 0;
    private int isOpen = 0;

    public int getTotal() {
        return total;
    }

    public void setTotal(int total) {
        this.total = total;
    }

    public int getValue() {
        return value;
    }

    public void setValue(int value) {
        this.value = value;
    }

    public int getIsOpen() {
        return isOpen;
    }

    public void setIsOpen(int isOpen) {
        this.isOpen = isOpen;
    }

    public BoxValueRespVO() {
    }

    public BoxValueRespVO(int total, int value, int isOpen) {
        this.total = total;
        this.value = value;
        this.isOpen = isOpen;
    }
}
