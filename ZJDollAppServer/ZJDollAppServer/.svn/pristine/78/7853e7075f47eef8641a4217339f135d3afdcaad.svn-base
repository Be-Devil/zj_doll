    background: url("http://lianai-image-sys.qiniudn.com/deposit/2.png");
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
    position: absolute;
    top: 2.8rem;
    left: 50%;
    transform: translate(-50%);
    width: 2.40rem;
    height: 0.64rem;
    border: none;
    margin-bottom: 1.72rem;
    font-family: PingFangSC-Medium;
    font-size: 0.32rem;
    color: #FFFFFF;
    line-height: 0.32rem;
}
.active1{
    background: url("http://lianai-image-sys.qiniudn.com/deposit/Rectangle%2011%20Copy@2x.png")!important;
    -webkit-background-size: 100% 100%!important;
    background-size: 100% 100%!important;
}
/*完成打钩*/
.accomplish{
    text-align: center;
}
.accomplish ul{
    margin-top: 0.74rem;
}
.accomplish ul li{
    font-family: PingFangSC-Regular;
    font-size: 0.3rem;
    color: #666666;
    line-height: 0.44rem;
}
.accomplish ul li img{
    width: 1.36rem;
    height: 1.36rem;
    margin-bottom: 0.32rem;
}
/*完成打钩结束*/
