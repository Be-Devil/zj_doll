<script>
    var num = 0;
    $(".top_btn").attr("class",function(){
        setInterval(function(){
            num++;
            if(num%3==0){
                $('.top_btn').addClass("top_btn_2");
            }else if(num%3==1) {
                $('.top_btn').addClass("top_btn_3");
                /*$('.top_btn').removeClass('top_btn');*/
                $('.top_btn').removeClass('top_btn_2');
            }
            else{
                $('.top_btn').addClass("top_btn");
                $('.top_btn').removeClass('top_btn_2');
                $('.top_btn').removeClass('top_btn_3');
            }
        },500)
    })
</script>'
