package com.imlianai.dollpub.app.modules.support.machine.vo;

import com.imlianai.dollpub.app.controller.vo.BaseSignReqVO;

/**
 * @author wurui
 * @create 2018-03-12 19:02
 **/
public class MachineMotorSpeedSetReqVO extends BaseSignReqVO {

    /**
     * 设备id
     */
    private String deviceId;

    /**
     * 机器id
     */
    private int busId;

    /**
     * 前后马达速度（取值范围（1-10））
     */
    private int frontAndBack;

    /**
     * 左右马达速度（取值范围（1-10））
     */
    private int leftAndRight;

    /**
     * 上下马达速度（取值范围（1-10））
     */
    private int upAndDown;

    public String getDeviceId() {
        return deviceId;
    }

    public void setDeviceId(String deviceId) {
        this.deviceId = deviceId;
    }

    public int getBusId() {
        return busId;
    }

    public void setBusId(int busId) {
        this.busId = busId;
    }

    public int getFrontAndBack() {
        return frontAndBack;
    }

    public void setFrontAndBack(int frontAndBack) {
        this.frontAndBack = frontAndBack;
    }

    public int getLeftAndRight() {
        return leftAndRight;
    }

    public void setLeftAndRight(int leftAndRight) {
        this.leftAndRight = leftAndRight;
    }

    public int getUpAndDown() {
        return upAndDown;
    }

    public void setUpAndDown(int upAndDown) {
        this.upAndDown = upAndDown;
    }
}
